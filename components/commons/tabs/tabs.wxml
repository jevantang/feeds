<t-tabs default-value="{{ value }}" sticky bind:change="onTabsChange" bind:click="onTabsClick" t-class="custom-tabs">
  <t-tab-panel label="{{ tabs.index }}" value="0" />
  <t-tab-panel label="{{ tabs.follow }}" value="1" />
  <t-tab-panel label="{{ tabs.nearby }}" value="2" />
</t-tabs>
